{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kaggleseries"
		},
		"KaggleLocalFileServer_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'KaggleLocalFileServer'"
		},
		"sinkkaggleseries_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sinkkaggleseries'"
		},
		"KaggleLocalFileServer_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\KaggleSeries"
		},
		"KaggleLocalFileServer_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "CHENG\\lchen"
		},
		"sinkkaggleseries_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kaggleseries.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kaggleseries_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sinkkaggleseries_accountKey": {
						"value": "[parameters('sinkkaggleseries_accountKey')]"
					},
					"KaggleLocalFileServer_password": {
						"value": "[parameters('KaggleLocalFileServer_password')]"
					},
					"sinkkaggleseries_properties_typeProperties_url": {
						"value": "[parameters('sinkkaggleseries_properties_typeProperties_url')]"
					},
					"KaggleLocalFileServer_properties_typeProperties_host": {
						"value": "[parameters('KaggleLocalFileServer_properties_typeProperties_host')]"
					},
					"KaggleLocalFileServer_properties_typeProperties_userId": {
						"value": "[parameters('KaggleLocalFileServer_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}